{
    "name": "toggle-format-on-save",
    "displayName": "Toggle Format on Save",
    "description": "Provides a command to toggle Format on Save",
    "version": "0.1.0",
    "publisher": "cbrevik",
    "engines": {
        "vscode": "^1.20.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cbrevik/toggle-format-on-save"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:toggle-format-on-save.toggle"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "toggle-format-on-save.toggle",
                "title": "Toggle Format on Save"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}